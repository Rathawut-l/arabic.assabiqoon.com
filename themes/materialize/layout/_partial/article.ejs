<div class="container">
    <div class="section">
        <!--   Icon Section   -->
        <div class="row">
            <div class="col s12 m9 l10">
                <h1><%= post.title %></h1>
                <hr>
                <%- post.content %>
            </div>
            <div class="col hide-on-small-only m3 l2">
                <div class="pinned">
                    <%- materialize_toc(post.content, { 'class': 'table-of-contents', list_number: false }) %>
                </div>
            </div>
        </div>
    </div>
</div>
